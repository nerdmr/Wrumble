{
  "name": "wrumble",
  "version": "0.9.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-reload": "ng serve --live-reload=false",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "ngrok:app": "ngrok http --host-header=rewrite 4200",
    "ngrok:p2p": "ngrok http --host-header=rewrite 3001",
    "deps": "npm run dependencies:ocr && npm run dependencies:link-preview",
    "deps:ocr": "docker-compose --project-directory ./dependencies/ocr/ up -d",
    "deps:link-preview": "npm run dev --prefix ./dependencies/url-preview",
    "deps:p2p": "npm run dev --prefix ./dependencies/p2p"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@angular/service-worker": "~13.1.0",
    "@primer/octicons": "^16.3.0",
    "markerjs2": "^2.18.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "tailwindcss": "^3.0.15",
    "typescript": "~4.5.2"
  }
}
